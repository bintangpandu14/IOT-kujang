    {
        "id": "7f2770c8f367827c",
        "type": "function",
        "z": "c390b250dd2f8427",
        "name": "function 4",
        "func": "msg.topic = `\nSELECT\n  MAX(timestamp) AS timestamp,\n  plant_name,\n  variety,\n  ROUND(MAX(CASE WHEN parameter_name = 'temperature' THEN value END), 2) AS temperature,\n  ROUND(MAX(CASE WHEN parameter_name = 'humidity' THEN value END), 2) AS humidity,\n  ROUND(MAX(CASE WHEN parameter_name = 'tds' THEN value END), 2) AS tds,\n  ROUND(MAX(CASE WHEN parameter_name = 'co2' THEN value END), 2) AS co2,\n  ROUND(MAX(CASE WHEN parameter_name = 'conductivity' THEN value END), 2) AS conductivity,\n  ROUND(MAX(CASE WHEN parameter_name = 'pH' THEN value END), 2) AS pH,\n  ROUND(MAX(CASE WHEN parameter_name = 'nitrogen' THEN value END), 2) AS nitrogen,\n  ROUND(MAX(CASE WHEN parameter_name = 'phosphorus' THEN value END), 2) AS phosphorus,\n  ROUND(MAX(CASE WHEN parameter_name = 'potassium' THEN value END), 2) AS potassium,\n  ROUND(MAX(CASE WHEN parameter_name = 'salinity' THEN value END), 2) AS salinity\nFROM sensor_logs\nGROUP BY plant_name, variety, DATE_FORMAT(timestamp, '%Y-%m-%d %H:%i:%s')\nORDER BY timestamp DESC\nLIMIT 50;\n\n`;\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
